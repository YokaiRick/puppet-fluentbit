# Managed by Puppet
[UPSTREAM]
  name  <%= $name %>

<% $nodes.each |$node_name, $node| { -%>
[NODE]
  name       <%= $node_name %>
  host       <%= $node['host'] %>
  port       <%= $node['port'] %>
  tls        <%= $node['tls'] %>
  tls.verify <%= $node['tls_verify'] %>

<% } -%>
# <%= $nodes.length %> nodes
